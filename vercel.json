{
    "version": 2,
    "builds": [
        {
            "src": "portfolio/wsgi.py",
            "use": "@vercel/python",
            "config": { 
                "runtime": "python3.11" 
            }
        }
    ],
    "routes": [ 
        {
            "src": "/(.*)",
            "dest": "portfolio/wsgi.py"
        }
    ]
}